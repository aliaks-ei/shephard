{
  "name": "shephard",
  "version": "1.0.0",
  "description": "Smart expenses wallet",
  "productName": "Shephard",
  "author": "Aliaksei Mazheika <aleksmozheiko94@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "lint:changed": "git diff --name-only -z --diff-filter=ACM main | grep -zZ -E '\\.(ts|tsx|vue|js|jsx|mjs|cjs|json|html|css)$' | xargs -0 eslint -c ./eslint.config.js --fix || true",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\"",
    "format:changed": "git diff --name-only -z --diff-filter=ACM main | grep -zZ -E '\\.(ts|tsx|vue|js|jsx|mjs|cjs|json|html|css)$' | xargs -0 prettier --write || true",
    "dev": "quasar dev -m pwa",
    "build": "quasar build -m pwa",
    "build:analyze": "quasar build -m pwa --mode analyze",
    "build:production": "NODE_ENV=production quasar build -m pwa",
    "preview": "quasar build -m pwa && quasar serve dist/pwa --port 4173",
    "postinstall": "quasar prepare",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "type-check": "vue-tsc --noEmit --composite false",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,vue,scss,html,md,json}": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\""
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@supabase/supabase-js": "^2.49.1",
    "@vueuse/core": "^13.2.0",
    "heic2any": "^0.0.4",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "register-service-worker": "^1.7.2",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@pinia/testing": "^1.0.1",
    "@prettier/plugin-oxc": "^0.0.4",
    "@quasar/app-vite": "^2.1.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.1.0",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.3.3",
    "supabase": "^2.51.0",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.9.0",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29",
    "workbox-build": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
